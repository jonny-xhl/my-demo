@page "{id:int?}"
@model PrivacyModel
@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>
<div class="card">
    <div class="card-body">
        <ul>
            <li>@Model?.PrivacyM?.Id</li>
            <li>@Model?.PrivacyM?.Name</li>
        </ul>
    </div>
</div>
